<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a href="<%= id_to_location( "#{id}-stacktrace" ) %>">
                    <i class="fa fa-link"></i>
                </a>

                <a data-toggle="collapse" href="#<%= id %>-stacktrace">
                    Stacktrace
                </a>
            </h4>
        </div>

        <div id="<%= id %>-stacktrace" class="panel-collapse collapse">
            <div class="panel-body">
                <%= erb 'issue/page/sink/trace/table.erb',
                        id:    "#{id}-stacktrace",
                        trace: trace,
                        page:  page
                %>

            </div>
        </div>
    </div>
</div>
